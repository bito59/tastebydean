<% unless @order_line.nil? %>
	$('#ol_<%= @order_line.id %>').fadeOut("slow",function(){ $(this).remove(); });
	console.log("Order_line was deleted. New total price : <%= j @order.subtotal.to_s %>");	
<% end %>
$("#subtotal").text("<%= j @order.subtotal.to_s + @order.price_unit %>");
$("#total_ttc").text("<%= j @order.total.to_s + @order.price_unit %>");
$('#flash_msg').append("<%= j render_flash %>").change();
console.log("cart updated");	
